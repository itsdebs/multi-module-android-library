{
  "name": "temp",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "./temp/src",
  "targets": {
    "build": {
      "executor": "@jnxplus/nx-gradle:build",
      "outputs": ["./temp/build"]
    },
    "lint": {
      "executor": "@jnxplus/nx-gradle:lint",
      "options": {
        "linter": "ktlint"
      }
    },
    "test": {
      "executor": "@jnxplus/nx-gradle:test"
    },
    "ktformat": {
      "executor": "@jnxplus/nx-gradle:ktformat"
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "baseBranch": "main",
        "preset": "conventional",
        "tagPrefix": "${projectName}@"
      }
    },
    "sync-bumped-version-properties": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node gradle-updater.js 'temp'"
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "temp@1.0.0",
        "notesFile": "./temp/CHANGELOG_LATEST.md"
      }
    },
    "release-sonatype": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'temp-release' && cd temp && sh ../gradlew publishToSonatype && cd .. && ./gradlew findSonatypeStagingRepository closeSonatypeStagingRepository && ./gradlew findSonatypeStagingRepository releaseSonatypeStagingRepository"
      }
    },
    "snapshot-release": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'temp-snap' && cd temp && sh ../gradlew publishToSonatype && cd .."
      }
    }
  },
  "tags": []
}
