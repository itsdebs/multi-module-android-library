{
  "name": "@rudderstack/rudder-multi-module-library",
  "version": "2.0.0",
  "description": "Rudder Android SDK monorepo",
  "keywords": [
    "android",
    "rudder",
    "kotlin"
  ],
  "author": "RudderStack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudderlabs/rudder-sdk-android.git"
  },
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-android/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-android#readme",
  "scripts": {
    "bump-version:monorepo": "npm version minor --git-tag-version false",
    "release": "nx affected --base=origin/master --target=version --parallel=1 --skipCommitTypes=docs,ci,chore",
    "release:github": "nx affected --target=github --parallel=1 --exclude=example --skipCommitTypes=docs,ci,chore"
  },
  "private": true,
  "dependencies": {
    "@babel/runtime": "7.21.0",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@rudderstack/core":"file:libs/core",
    "@rudderstack/android":"file:libs/android",
    "@rudderstack/api":"file:libs/api",
    "async-lock": "1.4.0",
    "core-js": "3.29.1",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-safe-area-context": "3.4.1",
    "react-native-screens": "3.20.0",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@digitalroute/cz-conventional-changelog-for-jira": "8.0.0",
    "@jscutlery/semver": "2.30.1",
    "@nrwl/cli": "15.8.9",
    "@nrwl/detox": "15.8.9",
    "@nrwl/eslint-plugin-nx": "15.8.9",
    "@nrwl/jest": "15.8.9",
    "@nrwl/linter": "15.8.9",
    "@nrwl/react-native": "15.8.9",
    "@nrwl/web": "15.8.9",
    "@nrwl/workspace": "15.8.9",
    "@react-native-async-storage/async-storage": "1.18.0",
    "@react-native-community/cli": "9.3.2",
    "@react-native-community/cli-platform-android": "9.3.1",
    "@react-native-community/cli-platform-ios": "9.3.0",
    "@rnx-kit/metro-resolver-symlinks": "0.1.27",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/jest-native": "5.4.1",
    "@testing-library/react-native": "11.5.0",
    "@types/async-lock": "1.4.0",
    "@types/jest": "29.5.0",
    "@types/node": "16.11.7",
    "@types/react": "18.0.30",
    "@types/react-native": "0.71.5",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "babel-jest": "29.5.0",
    "commitizen": "4.3.0",
    "commitlint": "17.5.1",
    "conventional-changelog-cli": "2.2.2",
    "conventional-github-releaser": "3.1.5",
    "detox": "20.6.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "8.0.3",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-circus": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-react-native": "18.0.0",
    "jest-sonar": "0.2.15",
    "lcov-result-merger": "4.1.0",
    "lint-staged": "13.2.0",
    "madge": "6.0.0",
    "metro": "0.76.0",
    "metro-babel-register": "0.76.0",
    "metro-react-native-babel-preset": "0.76.0",
    "metro-react-native-babel-transformer": "0.76.0",
    "metro-resolver": "0.74.0",
    "ngx-deploy-npm": "5.2.0",
    "nx": "15.8.9",
    "prettier": "2.8.7",
    "react-native-config": "1.5.0",
    "react-native-dotenv": "3.4.8",
    "react-native-svg": "13.8.0",
    "react-native-svg-transformer": "1.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "overrides": {
    "debug": "4.3.4",
    "dot-prop": "5.3.0",
    "jest": "29.5.0",
    "http-cache-semantics": "4.1.1",
    "semver-regex": "3.1.4",
    "shell-quote": "1.7.3",
    "trim-newlines": "3.0.1",
    "json5": "2.2.3",
    "unset-value": "2.0.1",
    "@nrwl/devkit": "15.8.9",
    "yaml": "2.2.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{json,js,md,ts,tsx}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "jiraMode": false,
      "jiraOptional": true,
      "skipScope": false,
      "scopes": [
        "core",
        "android",
        "api"
      ],
      "maxHeaderWidth": 120,
      "defaultScope": "@rudderstack/rudder-core",
      "defaultType": "feat"
    }
  }
}
